# Connection to SQL server which contains staging
# Example environment variables for staging_to_silver
# Copy this file to .env and adjust as needed

# Database
DRIVER=postgresql+psycopg2
HOST=localhost
PORT=5432
USER=sa
PASSWORD=your_password
DB=ggm

# Settings
SOURCE_SCHEMA=staging
TARGET_SCHEMA=silver
ASK_PASSWORD_IN_CLI=false

# Case normalization
# Allowed values: upper, lower; omit or leave empty for no normalization
TABLE_NAME_CASE=upper
COLUMN_NAME_CASE=