[database]
DRIVER=postgresql+psycopg2
HOST=localhost
PORT=5432
USER=sa
PASSWORD=SecureP@ss1!24323482349
DB=ggm
SCHEMA=ggm

[settings]
SOURCE_SCHEMA=staging
TARGET_SCHEMA=silver
# Of wachtwoord gevraagd wordt in CLI i.p.v. uit te lezen uit environment of .ini-config
ASK_PASSWORD_IN_CLI=false

# Case normalization for destination table names and projected column labels
# Allowed values: upper, lower, (leave empty for no normalization)
TABLE_NAME_CASE=upper
COLUMN_NAME_CASE=

[logging]
LOG_LEVEL=INFO
LOG_TO_FILE=False
LOG_FILE=logs/staging_to_silver.log
# Max. grootte (in bytes) van het logbestand voordat er een nieuw bestand wordt gestart (rotatie). Voorbeeld: 5.000.000 â‰ˆ 5 MB
LOG_ROTATE_BYTES=5000000
# Aantal oude logbestanden dat bewaard blijft bij rotatie (bijv. app.log.1 t/m app.log.N)
LOG_BACKUP_COUNT=3
# Optioneel: eigen opmaak van logregels. Laat weg voor standaard; voorbeeld toont tijd, level, logger en bericht
; LOG_FORMAT=%(asctime)s %(levelname)-8s [%(name)s] %(message)s